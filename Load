-- Module Scripts:

local fake_module_scripts = {}

do -- Login.ModuleScript
	local script = Instance.new('ModuleScript', Login)
	local function module_script()
		local whitelist = {
			["Jxn_Devv"] = true,
			["Aimlock_Jan"] = true,
		}
		
		return whitelist
		
	end
	fake_module_scripts[script] = module_script
end


-- Scripts:

local function YKEGNBW_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local whitelist = require(script.Parent.Parent.ModuleScript)
	local pass = "IfnVNkaAitN"
	local player = game.Players.LocalPlayer
	
	script.Parent.MouseButton1Click:Connect(function()
		if whitelist[player.Name] and script.Parent.Parent.TextBox.Text == pass then
			print(player.Name .. " Is whitelisted")
			script.Parent.Parent.TextBox.Visible = false
			script.Parent.Parent.TextButton.Visible = false
			script.Parent.Parent.Visible = false
			
			script.Parent.Parent.Parent.GameDetectedGUI:TweenPosition(
				UDim2.new(0.337, 0,0.39, 0),
				"Out",
				"Quad",
				.5,
				false
			)
			
		else
			player:Kick("You are not Whitelisted or you entered the wrong Password!")
		end
	end)
end
coroutine.wrap(YKEGNBW_fake_script)()
local function LXUGZN_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	while wait() do
		if game.PlaceId == 286090429 then
			script.Parent.Text = "Arsenal"
			
		elseif game.PlaceId ~= 286090429 then
			script.Parent.Text = "Please Enter a Supported Game!"
		end
	end
	
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.GameDetectedGUI:TweenPosition(
			UDim2.new(-0.281, 0,0.361, 0),
			"In",
			"Quad",
			.5,
			false
		)
		
		if game.PlaceId == 286090429 then
			loadstring(game:HttpGet(('https://raw.githubusercontent.com/xJxnDev/Hub/main/Script'),true))()
		else
			game.Players.LocalPlayer:Kick("Go in a Supported Game!")
		end
		
		
		script.Parent.Parent.Parent.Parent.ArsenalGUI.Visible = true
	end)
end
coroutine.wrap(LXUGZN_fake_script)()
